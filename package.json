  "peerDependencies": {
    "electron": "^10.1.3",
    "electron-log": "^4.2.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@blueprintjs/core": "^3.35.0",
    "@emotion/core": "^10.0.35",
    "@types/electron": "^1.6.10",
    "@types/js-yaml": "^3.12.5",
    "@types/react": "^16.9.49",
    "electron": "^10.1.3",
    "electron-log": "^4.3.0",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "@types/semver": "^7.3.4",
    "semver": "^7.3.2"
  }
}
