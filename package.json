    "@types/react": "17.0.53"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@blueprintjs/core": "~4.17.6",
    "@blueprintjs/popover2": "~1.13.10",
